{
  "name": "chat",
  "version": "1.0.0",
  "description": "chat",
  "engines": {
    "node": ">=14.0"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": ">=50%"
      }
    }
  },
  "jest": {
    "verbose": true
  },
  "scripts": {
    "test": "jest",
    "parcel": "parcel src/index.html --no-source-maps",
    "build": "parcel build src/index.html --no-source-maps --no-cache --public-url ./",
    "prebuild": "engine-version",
    "start": "node index.js",
    "lint": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaksimYakovlev/Chat.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "alias": {
    "core": "./src/core",
    "pages": "./src/pages",
    "store": "./src/store",
    "services": "./src/services",
    "utils": "./src/utils",
    "api": "./src/api"
  },
  "bugs": {
    "url": "https://github.com/MaksimYakovlev/Chat/issues"
  },
  "homepage": "https://github.com/MaksimYakovlev/Chat#readme",
  "dependencies": {
    "@babel/preset-typescript": "7.16.7",
    "@types/sinon": "10.0.11",
    "babel-core": "6.26.3",
    "express": "^4.17.3",
    "handlebars": "4.7.7",
    "ts-jest": "27.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "engine-version": "^1.0.2",
    "eslint": "8.12.0",
    "parcel": "2.4.0",
    "postcss": "8.4.12",
    "postcss-import": "14.1.0",
    "prettier": "2.6.2",
    "typescript": "4.6.2"
  }
}
